{
  "name": "ember-cognito-identity",
  "version": "3.3.0",
  "description": "Interact with AWS Cognito from your Ember app.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "git://github.com/fabscale/ember-cognito-identity.git",
  "author": "Francesco Novy <francesco@fabscale.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "run-prettier": "prettier --write \"**/*.{js,ts,scss}\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "MOCK_COGNITO=true ember serve",
    "test": "MOCK_COGNITO=true ember test",
    "test-all": "MOCK_COGNITO=true npm-run-all lint test:*",
    "test:all": "MOCK_COGNITO=true ember try:each",
    "test:one": "MOCK_COGNITO=true ember try:one",
    "test-coverage": "COVERAGE=true MOCK_COGNITO=true ember test",
    "release": "release-it",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "@ember/test-waiters": "^3.0.0",
    "@embroider/macros": "~0.43.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@types/amazon-cognito-auth-js": "^1.2.2",
    "amazon-cognito-identity-js": "^5.0.3",
    "ember-auto-import": "^2.0.1",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-typescript": "^4.0.0",
    "ember-concurrency": "^2.0.1",
    "ember-concurrency-ts": "^0.3.1"
  },
  "devDependencies": {
    "@ember/edition-utils": "~1.2.0",
    "@ember/optional-features": "~2.0.0",
    "@ember/test-helpers": "~2.4.2",
    "@embroider/test-setup": "~0.43.5",
    "@simple-dom/interface": "~1.4.0",
    "@types/ember": "~3.16.0",
    "@types/ember-qunit": "~3.4.8",
    "@types/ember__test-helpers": "~2.0.0",
    "@types/rsvp": "~4.0.3",
    "@typescript-eslint/eslint-plugin": "~4.30.0",
    "@typescript-eslint/parser": "~4.30.0",
    "babel-eslint": "~10.1.0",
    "broccoli-asset-rev": "~3.0.0",
    "ember-cli": "~3.28.0",
    "ember-cli-code-coverage": "~1.0.2",
    "ember-cli-dependency-checker": "~3.2.0",
    "ember-cli-dotenv": "~3.1.0",
    "ember-cli-inject-live-reload": "~2.1.0",
    "ember-cli-terser": "~4.0.2",
    "ember-cli-typescript-blueprints": "~3.0.0",
    "ember-disable-prototype-extensions": "~1.1.3",
    "ember-export-application-global": "~2.0.1",
    "ember-load-initializers": "~2.1.2",
    "ember-maybe-import-regenerator": "~1.0.0",
    "ember-modifier": "~2.1.2",
    "ember-page-title": "~6.2.2",
    "ember-qunit": "~5.1.4",
    "ember-resolver": "~8.0.2",
    "ember-source": "~3.28.0",
    "ember-source-channel-url": "~3.0.0",
    "ember-template-lint": "~3.6.0",
    "ember-truth-helpers": "~3.0.0",
    "ember-try": "~1.4.0",
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-ember": "~10.5.4",
    "eslint-plugin-ember-es6-class": "~0.0.1",
    "eslint-plugin-ember-suave": "~2.0.1",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-qunit": "^6.2.0",
    "loader.js": "~4.7.0",
    "prettier": "~2.3.2",
    "qrcode": "~1.4.4",
    "qunit": "~2.16.0",
    "qunit-dom": "~2.0.0",
    "release-it": "~14.11.5",
    "release-it-lerna-changelog": "~3.1.0",
    "typescript": "~4.4.3",
    "webpack": "~5.53.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "12.18.3",
    "yarn": "1.22.5"
  }
}
