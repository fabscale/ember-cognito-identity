{
  "name": "ember-cognito-identity",
  "version": "1.5.3",
  "description": "Interact with AWS Cognito from your Ember app.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "git://github.com/fabscale/ember-cognito-identity.git",
  "author": "Francesco Novy <francesco@fabscale.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "run-prettier": "prettier --write \"**/*.{js,ts,scss}\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "ember test",
    "test-all": "npm-run-all lint test:*",
    "test:all": "ember try:each",
    "test:one": "ember try:one",
    "test-coverage": "COVERAGE=true ember test",
    "release": "release-it",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "@types/amazon-cognito-auth-js": "^1.2.2",
    "amazon-cognito-identity-js": "^4.5.9",
    "ember-auto-import": "^1.10.1",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-typescript": "^4.0.0",
    "ember-concurrency": "^1.1.7",
    "ember-concurrency-decorators": "^2.0.0",
    "@ember/test-waiters": "^2.3.2",
    "pretender": "^3.4.0"
  },
  "devDependencies": {
    "@ember/edition-utils": "~1.2.0",
    "@ember/optional-features": "~2.0.0",
    "@ember/test-helpers": "~2.2.3",
    "@embroider/test-setup": "~0.36.0",
    "@release-it/conventional-changelog": "~2.0.0",
    "@simple-dom/interface": "~1.4.0",
    "@types/ember": "~3.16.0",
    "@types/ember-qunit": "~3.4.8",
    "@types/ember__test-helpers": "~1.7.0",
    "@types/rsvp": "~4.0.3",
    "babel-eslint": "~10.1.0",
    "broccoli-asset-rev": "~3.0.0",
    "ember-cli": "~3.24.0",
    "ember-cli-code-coverage": "~1.0.2",
    "ember-cli-dependency-checker": "~3.2.0",
    "ember-cli-dotenv": "~3.1.0",
    "ember-cli-inject-live-reload": "~2.0.2",
    "ember-cli-terser": "~4.0.1",
    "ember-cli-typescript-blueprints": "~3.0.0",
    "ember-disable-prototype-extensions": "~1.1.3",
    "ember-export-application-global": "~2.0.1",
    "ember-load-initializers": "~2.1.2",
    "ember-maybe-import-regenerator": "~0.1.6",
    "ember-page-title": "~6.2.0",
    "ember-qunit": "~5.1.1",
    "ember-resolver": "~8.0.2",
    "ember-source": "~3.25.1",
    "ember-source-channel-url": "~3.0.0",
    "ember-template-lint": "~2.18.0",
    "ember-try": "~1.4.0",
    "eslint": "~7.19.0",
    "eslint-config-prettier": "~8.1.0",
    "eslint-plugin-ember": "~10.2.0",
    "eslint-plugin-ember-es6-class": "~0.0.1",
    "eslint-plugin-ember-suave": "~2.0.1",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~3.3.1",
    "loader.js": "~4.7.0",
    "prettier": "~2.2.1",
    "qunit": "~2.14.0",
    "qunit-dom": "~1.6.0",
    "release-it": "~14.2.1",
    "typescript": "~4.2.2"
  },
  "engines": {
    "node": ">= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "12.18.3",
    "yarn": "1.22.5"
  }
}
